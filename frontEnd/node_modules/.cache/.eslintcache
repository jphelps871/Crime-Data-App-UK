 objects and their quantities (how many times they appear)\n    const crimeQuantity = allCrimeData.reduce((allCrimes, crime) => {\n      if (crime.category in allCrimes) {\n        allCrimes[crime.category]++;\n      } else {\n        allCrimes[crime.category] = 1;\n      }\n      return allCrimes;\n    }, {});\n\n    // return an array from object\n    setCrimeData(Object.entries(crimeQuantity));\n    setLoading(false);\n  } catch (e) {\n    setStreetNameFromData(\"Sorry, there is no information on this location\");\n\n    // return crimeData to nothing\n    setCrimeData([]);\n  }\n};\n\nexport { getAllData };\n",{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":1,"column":17,"nodeType":"78","messageId":"79","endLine":1,"endColumn":26},{"ruleId":"76","severity":1,"message":"80","line":1,"column":28,"nodeType":"78","messageId":"79","endLine":1,"endColumn":36},{"ruleId":"76","severity":1,"message":"81","line":6,"column":10,"nodeType":"78","messageId":"79","endLine":6,"endColumn":18},{"ruleId":"76","severity":1,"message":"82","line":6,"column":20,"nodeType":"78","messageId":"79","endLine":6,"endColumn":31},{"ruleId":"83","severity":1,"message":"84","line":9,"column":13,"nodeType":"85","endLine":20,"endColumn":4},{"ruleId":"83","severity":1,"message":"86","line":19,"column":6,"nodeType":"87","endLine":19,"endColumn":13,"suggestions":"88"},{"ruleId":"83","severity":1,"message":"86","line":21,"column":6,"nodeType":"87","endLine":21,"endColumn":13,"suggestions":"89"},{"ruleId":"76","severity":1,"message":"90","line":1,"column":8,"nodeType":"78","messageId":"79","endLine":1,"endColumn":13},{"ruleId":"76","severity":1,"message":"77","line":1,"column":17,"nodeType":"78","messageId":"79","endLine":1,"endColumn":26},{"ruleId":"76","severity":1,"message":"80","line":1,"column":28,"nodeType":"78","messageId":"79","endLine":1,"endColumn":36},"no-native-reassign",["91"],"no-negated-in-lhs",["92"],"no-unused-vars",