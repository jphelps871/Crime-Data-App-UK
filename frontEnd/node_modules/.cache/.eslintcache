;\n\n  // run function to store data\n  const userInputInformation = (event) => {\n    setRegisterDetails({\n      ...registerDetails,\n      [event.target.name]: event.target.value,\n    });\n  };\n\n  return (\n    <form className=\"sign-up\" onSubmit={submitForm}>\n      <h2>Sign In</h2>\n      <label htmlFor=\"email\">Email</label>\n      <input type=\"email\" name=\"email\" onChange={userInputInformation} />\n      <label htmlFor=\"password\">Password</label>\n      <input type=\"password\" name=\"password\" onChange={userInputInformation} />\n      <p>{success}</p>\n      <button>Sign In</button>\n    </form>\n  );\n};\n\nexport default SignIn;\n",{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","severity":1,"message":"62","line":47,"column":6,"nodeType":"63","endLine":47,"endColumn":29,"suggestions":"64"},{"ruleId":"65","severity":1,"message":"66","line":1,"column":17,"nodeType":"67","messageId":"68","endLine":1,"endColumn":25},{"ruleId":"65","severity":1,"message":"69","line":1,"column":27,"nodeType":"67","messageId":"68","endLine":1,"endColumn":35},{"ruleId":"61","severity":1,"message":"70","line":21,"column":6,"nodeType":"63","endLine":21,"endColumn":13,"suggestions":"71"},{"ruleId":"61","severity":1,"message":"70","line":19,"column":6,"nodeType":"63","endLine":19,"endColumn":13,"suggestions":"72"},"no-native-reassign",["73"],"no-negated-in-lhs",["74"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getAllData'. Either include it or remove the dependency array.","ArrayExpression",["75"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'UseState' is defined but never used.","React Hook useEffect has a missing dependency: 'registerUser'. Either include it or remove the dependency array.",["76"],["77"],"no-global-assign","no-unsafe-negation",{"desc":"78","fix":"79"},{"desc":"80","fix":"81"},{"desc":"80","fix":"82"},"Update the dependencies array to be: [getAllDa